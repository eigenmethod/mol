$mol_switcher_demo_enabled $mol_switcher
	value?val <=> color?val \red
	options *
		red \Red
		green \Green
		blue \Blue

$mol_switcher_demo_disabled $mol_switcher
	value?val <=> color?val \red
	enabled false
	options *
		red <= optionRed @ \Red
		green <= optionGreen @ \Green
		blue <= optionBlue @ \Blue
