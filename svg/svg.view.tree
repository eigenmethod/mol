$mol_svg $mol_view
	dom_name \svg
	dom_name_space \http://www.w3.org/2000/svg

$mol_svg_root $mol_view
	dom_name \svg
	attr * viewBox <= viewBox \0 0 100 100 

$mol_svg_path $mol_svg
	dom_name \path
	attr * d <= geometry \

$mol_svg_circle $mol_svg
	dom_name \circle
	attr *
		r <= radius \
		cx <= pos_x \
		cy <= pos_y \
