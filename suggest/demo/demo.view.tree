$mol_suggest_demo_base $mol_suggest
	suggests <= query_suggests /
	value?val <=> query?val \

$mol_suggest_demo_empty $mol_suggest
	focused false
	value \
	suggests /

$mol_suggest_demo_opened $mol_suggest
	hint @ \Name
	value \Ivan
	focused true
	selected_row 2
	suggests /
		<= suggest1 @ \Evgeny Ivanov
		<= suggest2 @ \Alexander Ivanov
		<= suggest3 @ \Dmitriy Ivanov
		<= suggest4 @ \Yuriy Ivanov
		<= suggest5 @ \Nataliya Ivanov
